{
  "name": "svelte-package-ignores-tsconfig",
  "version": "0.0.1",
  "scripts": {
    "svelte-build": "svelte-kit sync && svelte-package",
    "tsc-build": "svelte-kit sync && tsc"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
